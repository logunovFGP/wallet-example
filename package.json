{
  "name": "sequelize-models",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./server",
    "db:makemigrations": "node ./node_modules/sequelize-auto-migrations/bin/makemigration.js --migrations-path=server/dao/migrations --models-path=server/dao/models",
    "db:migrate": "sequelize-cli db:migrate --options-path=cli-options.json",
    "db:migrate:create": "sequelize-cli db:create --options-path=cli-options.json && npm run db:migrate",
    "db:seed": "sequelize-cli db:seed:all --options-path=cli-options.json",
    "db:seed:undo": "sequelize-cli db:seed:undo:all --options-path=cli-options.json",
    "db:drop:BIG_CONFIRM_I_AM_SURE_TO_DELETE_DATABASE": "sequelize-cli db:drop --options-path=cli-options.json",
    "db:seed:admin": "sequelize-cli db:seed --seed 999-admin --options-path=cli-options.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.2",
    "@ethereumjs/tx": "^3.3.0",
    "@hapi/joi": "^17.1.1",
    "@polkadot/api": "latest",
    "@polkadot/keyring": "latest",
    "@polkadot/util-crypto": "latest",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bitcoinjs-lib": "^5.2.0",
    "bluebird": "^3.7.2",
    "bn.js": "^5.2.0",
    "camelcase": "^6.2.0",
    "cardano-wallet-js": "^1.2.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "decamelize": "^5.0.0",
    "dotenv": "^8.2.0",
    "ethers": "^5.4.4",
    "express": "^4.17.1",
    "glob": "latest",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "ripple-lib": "^1.10.0",
    "sequelize": "^5.21.13",
